{
  "name": "Piano-Hands",
  "descriptions": "Object tagging of right and left hands on a piano background",
  "homepage": "https://github.com/AmitMY/piano-hands",
  "data": {
    "image": {
      "type": "file",
      "parameters": {
        "type": "image",
        "extension": "jpg"
      }
    },
    "annotation": {
      "type": "file",
      "parameters": {
        "extension": "xml"
      }
    },
    "objects": {
      "type": "list",
      "parameters": {
        "type": "object",
        "parameters": {
          "category": {
            "type": "text"
          },
          "polygon": {
            "type": "polygon"
          }
        }
      }
    }
  },
  "versions": [
    {
      "date": "2019-08-11",
      "version": "1.0",
      "url": "https://github.com/AmitMY/piano-hands/releases/download/1.0/dataset.zip"
    }
  ]
}